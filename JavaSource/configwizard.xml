<?xml version="1.0"?>
<servlet_config>
	<pages>
		<page>
			<id>config2</id>
			<jsp>/carrier/configwiz2.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.ConfigurationWizardPage</pagebean>
		</page>
		<page>
			<id>config2.invalid.graphicfile</id>
			<jsp>/carrier/configwiz2.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.ConfigWizLogoInvalidPage</pagebean>
		</page>
		<page>
			<id>config3</id>
			<jsp>/carrier/dlconfig.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.ConfigurationWizardPage</pagebean>
		</page>
		<page>
			<id>config4</id>
			<jsp>/carrier/adv_options.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.ConfigurationWizardPage</pagebean>
		</page>
		<page>
			<id>config5</id>
			<jsp>/carrier/download_alert.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.ConfigurationWizardPage</pagebean>
		</page>
		<page>
			<id>config_end</id>
			<jsp>/carrier/configwizcomplete.jsp</jsp>
			<displaybean>connective.teamup.download.beans.CompanyConfigDisplayBean</displaybean>
		</page>
	</pages>
	<actions>
		<action>
			<id>config2_back</id>
			<alias>config2</alias>
		</action>
		<action>
			<id>config2_next</id>
			<actionbean>connective.teamup.download.actions.SaveConfigWizardProps</actionbean>
		</action>
		<action>
			<id>config3_back</id>
			<alias>config2</alias>
		</action>
		<action>
			<id>dlconfigsave</id>
			<actionbean>connective.teamup.download.actions.SaveDownloadConfig</actionbean>
		</action>
		<action>
			<id>config4_back</id>
			<alias>config3</alias>
		</action>
		<action>
			<id>config5_back</id>
			<alias>config4</alias>
		</action>
		<action>
			<id>advopt_save</id>
			<actionbean>connective.teamup.download.actions.SaveCarrierAdvancedOptions</actionbean>
		</action>
		<action>
			<id>dlalert_save</id>
			<actionbean>connective.teamup.download.actions.SaveDLAlertProps</actionbean>
		</action>
	</actions>
	<menubar>
		<menu>
			<text>Help</text>
			<item>
				<text>Contact Ebix,Inc</text>
				<action>mailto:techsupport@connective-edi.com?subject=[TAG:CarrierShortName] Download Configuration</action>
			</item>
			<item>
				<text>Help</text>
				<action>link:/teamupdl/help/Help_Config_FrameSet.jsp</action>
			</item>
		</menu>
	</menubar>
</servlet_config>
<?xml version="1.0"?>
<servlet_config>
	<pages>
		<page>
			<id>batch.import</id>
			<jsp>/carrier/import.jsp</jsp>
			<displaybean>connective.teamup.download.beans.ImportDisplayBean</displaybean>
			<pagebean>connective.teamup.download.pages.BatchImportPage</pagebean>
		</page>
		<page>
			<id>dlreport_complete</id>
			<jsp>/carrier/dlreport.jsp</jsp>
		</page>
		<page>
			<id>menu.about</id>
			<jsp>/carrier/about.jsp</jsp>
			<displaybean>connective.teamup.download.beans.AppInfoDisplayBean</displaybean>
		</page>
	</pages>
	<actions>
		<action>
			<id>batch_import</id>
			<alias>batch.import</alias>
		</action>
		<action>
			<id>download_report</id>
			<actionbean>connective.teamup.download.actions.DownloadReport</actionbean>
		</action>
		<action>
			<id>import_complete</id>
			<actionbean>connective.teamup.download.actions.ImportComplete</actionbean>
		</action>
		<action>
			<id>import_report</id>
			<actionbean>connective.teamup.download.actions.ImportReport</actionbean>
		</action>
		<action>
			<id>import_start</id>
			<actionbean>connective.teamup.download.actions.ImportBatch</actionbean>
		</action>
		<action>
			<id>menu_about</id>
			<alias>menu.about</alias>
		</action>
		<action>
			<id>cl_purge</id>
			<actionbean>connective.teamup.download.actions.PurgeManual</actionbean>
		</action>
		<action>
			<id>uploadfile</id>
			<actionbean>connective.teamup.download.actions.UploadFile</actionbean>
		</action>
		<action>
			<id>xml_import_check</id>
			<actionbean>connective.teamup.download.actions.ImportXmlCheck</actionbean>
		</action>
		<action>
			<id>xml_import_pcheck</id>
			<actionbean>connective.teamup.download.actions.ImportPolicyXmlCheck</actionbean>
		</action>
		<action>
			<id>xml_import_file</id>
			<actionbean>connective.teamup.download.actions.ImportXmlFile</actionbean>
		</action>
		<action>
			<id>xml_import_pfile</id>
			<actionbean>connective.teamup.download.actions.ImportPolicyXmlFile</actionbean>
		</action>
	</actions>
	<menubar>
		<menu>
			<text>Help</text>
			<item>
				<text>About</text>
				<action>menu_about</action>
			</item>
			<item>
				<text>Contact Ebix,Inc</text>
				<action>mailto:techsupport@connective-edi.com?subject=[TAG:CarrierShortName] Download</action>
			</item>
			<item>
				<text>Help</text>
				<action>link:/teamupdl/help/Help_Carrier_FrameSet.jsp</action>
			</item>
		</menu>
	</menubar>
</servlet_config>